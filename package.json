    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mongoose": "^6.0.2",
    "nanoid": "^3.1.23",
    "next": "^11.0.1",
    "next-iron-session": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-emoji-render": "^1.2.4",
    "react-icons": "^4.1.0",
    "react-sortable-hoc": "^1.11.0",
    "redaxios": "^0.4.1"

{
  "name": "cesium.link",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all develop",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --fix --ignore-path .gitignore .",
    "fmt": "npm-run-all format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{css,js,json,md,mdx}'",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "eslint --ignore-path .gitignore .",
    "test:format": "prettier --ignore-path .gitignore --check './**/*.{css,js,json,md,mdx}'"
  },
  "dependencies": {
    "next": "latest",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2"
  }
}
